@model IEnumerable<Corso>

<h2>Lista Corsi</h2>

<table>
    <tr style="font-weight: bold;"><td>Id</td><td>Difficolt&agrave;</td><td>Prezzo Base</td><td>Titolo</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.Id</td><td>@item.Difficolta</td><td>@item.PrezzoBase</td><td>@item.Titolo</td></tr>
    }
</table>

