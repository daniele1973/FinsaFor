<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>

    <nav class="navbar navbar-inverse navbar-fixed-top">

        <div class="container">

            <div class="navbar-collapse collapse">

                <ul class="nav navbar-nav">

                    <li class="navbar-item"><a asp-controller="Corsi" asp-action="Corsi">Corsi</a></li>
                    <li class="navbar-item"><a asp-controller="EdizioniCorsi" asp-action="Visualizza">Edizioni Corsi</a></li>
                    <li class="navbar-item"><a asp-controller="Allievi" asp-action="Index">Allievi</a></li>
                    <li class="navbar-item"><a asp-controller="CorsiAllievi" asp-action="CorsiAllievi">CorsiAllievi</a></li>


                </ul>


              

               

            </div>

        </div>

    </nav>


    <div class="container body-content">
        <div class="row row-eq-height">
            <div class="col-md-3">
                <p class="lead">
                    <img class="img-responsive" alt="Ente F.IRE" src="~/Images/logo_fire.png" />
                </p>
            </div>
            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>


